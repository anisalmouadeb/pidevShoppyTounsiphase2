@model IEnumerable<pidevShoppyTounsi.Models.DiscountToken>

@{
    ViewBag.Title = "MyDiscountTokens";
}

<h2>My Discount Tokens</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.validity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.used)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.value)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.validity)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.used)
    </td>
    <td>
        @Html.ActionLink("Details", "Details", new { id = item.discountTokenId }) |
    </td>
</tr>
}

</table>
