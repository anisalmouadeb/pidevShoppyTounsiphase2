@using pidevShoppyTounsi.Controllers
@{
    ViewBag.Title = "About";
}

@if (LoginController.TokenConnect != "")
{ Layout = "~/Views/Shared/ConnectedClient.cshtml"; }
else
{ Layout = "~/Views/Shared/NotConnectedClient.cshtml"; }

<nav class="breadcrumb-nav">
    <div class="container">
        <ul class="breadcrumb">
            <li><a href="demo1.html"><i class="d-icon-home"></i></a></li>
            <li>About us</li>
        </ul>
    </div>
</nav>

<div class="page-header pl-4 pr-4" style="  background-image: url(../assets/images/about-us.jpg)">
